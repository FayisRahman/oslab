breakpoint;

alias functionNum R1;
alias currentPID R2;


//Accquire Terminal
if(functionNum == 8) then

    while([TERMINAL_STATUS_TABLE + 0] == 1) do

        [PROCESS_TABLE + currentPID * 16 + 4] = WAIT_TERMINAL;

        multipush(R0,R1,R2,R3,R4,R5,R6,R7,R8,R9,R10,R11,R12,R13,R14,R15);

        call MOD_5;

        multipop(R0,R1,R2,R3,R4,R5,R6,R7,R8,R9,R10,R11,R12,R13,R14,R15);

    endwhile;

    [TERMINAL_STATUS_TABLE + 0] = 1; //status
    
    [TERMINAL_STATUS_TABLE + 1] = currentPID; //current running process


    return;

endif;


//Release Terminal

if(functionNum == 9) then

    if(currentPID != [TERMINAL_STATUS_TABLE + 1]) then

        R0 = -1;

        return;
    
    endif;

    [TERMINAL_STATUS_TABLE + 0] = 0; //status

    alias i R3;

    i = 0;

    while(i < 16) do
        
        if( [PROCESS_TABLE + i * 16 + 4] == WAIT_TERMINAL ) then

            [PROCESS_TABLE + i * 16 + 4] = READY;

        endif;

        i = i + 1;

    endwhile;

    R0 = 0;

    return;

endif;




